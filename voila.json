{
    "VoilaConfiguration": {
      "extension_language_mapping": {
        ".py": "python",
        ".jl": "julia"
      },
      "language_kernel_mapping": {
        "python": "python3"
      },
      "template": "default",
      "strip_sources": false,
      "enable_nbextensions": true,
      "theme": "dark",
      "ExecutePreprocessor": {
        "enabled": true,
        "timeout": 180
      },
      "TagRemovePreprocessor": {
        "remove_cell_tags": ["remove_cell"],
        "remove_all_outputs_tags": ["hide_output"],
        "remove_input_tags": ["hide_input"]
      }
    }
  }